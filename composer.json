{
    "name": "cohensive/laravel-amp",
    "description": "Package that helps you set up AMP (Accelerated Mobile Pages) using Laravel",
    "keywords": [
        "wearejust",
        "cohensive",
        "laravel-amp"
    ],
    "homepage": "https://github.com/KaneCohen/laravel-amp",
    "license": "MIT",
    "authors": [
        {
            "name": ":Cees van Egmond",
            "email": "cees.vanegmond@wearejust.com",
            "homepage": "https://wearejust.com",
            "role": "Developer"
        },
        {
            "name": "Kane Cohen",
            "email": "kanecohen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cohensive\\Amp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cohensive\\Amp\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cohensive\\Amp\\AmpServiceProvider"
            ],
            "aliases": {
                "AmpRouter": "Cohensive\\Amp\\AmpRouteFacade"
            }
        }
    }
}
